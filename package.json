{
  "name": "ember-froala-editor",
  "version": "4.0.16",
  "description": "An ember-cli addon that properly wraps the Froala WYSIWYG Editor for use in Ember.js",
  "keywords": [
    "ember-addon",
    "froala",
    "froala-editor",
    "froala-wysiwyg",
    "froala-wysiwyg-editor",
    "ember",
    "html",
    "rich",
    "text",
    "editor",
    "wysiwyg"
  ],
  "repository": "github:froala/ember-froala-editor",
  "homepage": "https://github.com/froala/ember-froala-editor#readme",
  "bugs": "https://github.com/froala/ember-froala-editor/issues",
  "license": "MIT",
  "contributors": [
    "Ryan Panning",
    "Stefan Neculai",
    "Michael Donaldson",
    "Diana Neculai",
    "Laura Lebovic",
    "Richard Livsey",
    "Raphael Nunes dos Santos",
    "Keita Urashima"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all --print-name \"lint\" \"test:*\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.5",
    "@embroider/macros": "^1.10.0",
    "@glimmer/component": "^1.1.2",
    "ember-auto-import": "^2.5.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-version-checker": "^5.1.2",
    "froala-editor": "^4.0.16"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^1.8.3",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.9.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-deprecation-workflow": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-update": "^1.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^6.1.1",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.9.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.18.2",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ember": "^11.4.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "qunit": "^2.19.3",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://github.com/froala/ember-froala-editor#readme",
    "versionCompatibility": {
      "ember": ">=3.20.0"
    }
  }
}
