{
  "name": "ember-froala-editor",
  "version": "4.1.3",
  "description": "An ember-cli addon that properly wraps the Froala WYSIWYG Editor for use in Ember.js",
  "keywords": [
    "ember-addon",
    "froala",
    "froala-editor",
    "froala-wysiwyg",
    "froala-wysiwyg-editor",
    "ember",
    "html",
    "rich",
    "text",
    "editor",
    "wysiwyg"
  ],
  "repository": "github:froala/ember-froala-editor",
  "homepage": "https://github.com/froala/ember-froala-editor#readme",
  "bugs": "https://github.com/froala/ember-froala-editor/issues",
  "license": "MIT",
  "contributors": [
    "Ryan Panning",
    "Stefan Neculai",
    "Michael Donaldson",
    "Diana Neculai",
    "Laura Lebovic",
    "Richard Livsey",
    "Raphael Nunes dos Santos",
    "Keita Urashima"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "ember build --environment=production",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.23.5",
    "@ember/render-modifiers": "^2.1.0",
    "@embroider/macros": "^1.13.1",
    "@glimmer/component": "^1.1.2",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "froala-editor": "^4.1.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.3",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-cli": "~5.4.1",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-update": "^1.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.2",
    "ember-try": "^3.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^3.0.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^2.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "ember-source": ">=3.20.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://github.com/froala/ember-froala-editor#readme",
    "versionCompatibility": {
      "ember": ">=3.20.0"
    }
  }
}
